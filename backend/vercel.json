{
  "version": 2,
  "builds": [
    { "src": "api/**/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/food", "dest": "/api/food.js" },
    { "src": "/api/user", "dest": "/api/user.js" },
    { "src": "/api/cart", "dest": "/api/cart.js" },
    { "src": "/api/order", "dest": "/api/order.js" },
    { "src": "/api/contact", "dest": "/api/contact.js" },
    { "src": "/images/(.*)", "dest": "/uploads/$1" }
  ]
}
